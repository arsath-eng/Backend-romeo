{
  "name": "hrm-apis",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "clear": "jest --clearCache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --setupFiles dotenv/config --config ./test/jest-e2e.json --watch",
    "run": "nodemon main",
    "typeorm": "npm run build && npx typeorm -d dist/db/database.config.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@hubspot/api-client": "^11.1.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.3",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/typeorm": "^9.0.1",
    "@nextnm/nestjs-ses": "^0.0.2",
    "amazon-cognito-identity-js": "^5.2.12",
    "archiver": "^7.0.1",
    "argon2": "^0.30.3",
    "aws-sdk": "^2.1181.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.0.1",
    "cache-manager": "^5.2.1",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "directory-tree": "^3.2.1",
    "dns": "^0.1.2",
